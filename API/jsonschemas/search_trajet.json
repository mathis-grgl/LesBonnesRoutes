{
    "type": "object",
    "properties": {
        "idConducteur": {
            "type": "integer",
            "minimum": 1
        },
        "dateDepart": {
            "type": "string",
            "pattern": "^[0-9]{4}-[0-9]{2}-[0-9]{2}T[0-9]{2}:[0-9]{2}$"
        }, 
        "nbPlaces": {
            "type": "integer",
            "minimum": 0
        },
        "prix": {
            "type": "integer",
            "minimum": 0
        }, 
        "nbPlacesRestantes": {
            "type": "integer",
            "minimum": 0
        }, 
        "statusTrajet": {
            "type": "string",
            "pattern": "^Complet|A pourvoir|En cours|Termine$"
        },
        "villeDepart": {
            "type": "integer",
            "minimum": 1,
            "maximum": 54
        },
        "villeArrivee": {
            "type": "integer",
            "minimum": 1,
            "maximum": 54
        }
    },
    "additionalProperties": false
}